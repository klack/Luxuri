            commands.command.txt="M155 S5" //Update temp slower
            click luxSend,1
            commands.command.txt="M107" //Turn off fan.
            click luxSend,1
        
Hotspot luxController
    Attributes
        ID               : 11
        Scope            : local
        Dragging         : 0
        Send Component ID: disabled
        Opacity          : 127
        x coordinate     : 117
        y coordinate     : 65465
        Width            : 106
        Height           : 17
        Effect           : load
    
    Events
        Touch Press Event
            ///////////////////////////////////////////////////////////////////////////////
            /// @fn luxController 1
            /// @brief Program loop
            ///////////////////////////////////////////////////////////////////////////////
            if(state.txt=="null")
            {
                thsp=0 //Disable sleep
                state.txt="init"
            }
            if(state.txt=="init")
            {
                click checkHeaters,1
            }
            if(state.txt=="cooling")
            {
                if(printer.T0Temp.val<40&&printer.T1Temp.val<40)
                {
                    state.txt="ready"
                }
            }
            if(state.txt=="ready")
            {
                click tune,1
                state.txt="tuning"
            }
            if(state.txt=="tuning")
            {
                oProgress.val=cycle.val*100/totalCycles.val
                if(oProgress.val==100)
                {
                    state.txt="complete"
                    click luxView,1
                    click finish,1
                }
            }
            click luxView,1
        
        Touch Release Event
            ///////////////////////////////////////////////////////////////////////////////
            /// @fn luxController 0
            /// @brief Custom on page callback handling
            ///////////////////////////////////////////////////////////////////////////////
            if(ReturnVal==1) //Handle start confirmation diag
            {
                state.txt="init"
            }else
            {
                page ReturnPage
            }
        
