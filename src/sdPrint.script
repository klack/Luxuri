            ///////////////////////////////////////////////////////////////////////////////
            parse.lineMatch.txt="End file list"
            if(parse.buffer.txt==parse.lineMatch.txt)
            {
                gettingFiles.val=0 //Getting file state is off, do not listen for filenames
