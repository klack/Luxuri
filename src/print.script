Hotspot luxHandleSleep
    Attributes
        ID               : 22
        Scope            : local
        Dragging         : 0
        Send Component ID: disabled
        Opacity          : 127
        x coordinate     : 255
        y coordinate     : 65511
        Width            : 106
        Height           : 17
        Effect           : load
    
    Events
        Touch Press Event
            ///////////////////////////////////////////////////////////////////////////////
            /// @fn luxHandleSleep
            /// @brief Handle a finished print while LCD was asleep
            ///////////////////////////////////////////////////////////////////////////////
            if(printer.sdPrinting.val==0&&printer.printState.txt=="printing")
            {
                commands.queue.txt+="" //Clear invalid commands
                click luxSend,0
                commands.queue.txt+="M31\r" //Get print time
                click luxSend,0
                printer.printState.txt="finishing"
                click luxView,1
                parse.callback.txt="luxHandleSleep:finished"
                click luxSetCallback,1
            }
        
