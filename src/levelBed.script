            {
                commands.queue.txt+="T"
                covx ReturnVal,vars.s.txt,0,0
                commands.queue.txt+=vars.s.txt
                commands.queue.txt+="\r"
                commands.queue.txt+="M114\r"
                click luxSend,0
            }
        
