            /// @fn luxSwapEx 1
            /// @brief Swaps between T0 and T1
            ///////////////////////////////////////////////////////////////////////////////
            if(Tool==0)
            {
                diagPrompt.tMessage.txt="Switch to Extruder 2?"
                ReturnVal=1
            }else
            {
                diagPrompt.tMessage.txt="Switch to Extruder 1?"
                ReturnVal=0
            }
            ReturnObj=5
            page diagPrompt
        
        Touch Release Event
